{
  "name": "music-sharing-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "test": "jest --watch --verbose",
    "remote:restart": "bash ./deploy/restart.sh",
    "remote:destroy": "bash ./deploy/delete.sh",
    "remote:start": "bash ./deploy/start.sh",
    "remote:deploy": "bash ./deploy/pull.sh && bash ./deploy/deploy.sh",
    "lint:check": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts ./",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@upstash/ratelimit": "^0.1.5",
    "@upstash/redis": "^1.18.4",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "knex": "^2.3.0",
    "next": "13.0.6",
    "pg": "^8.8.0",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.40.0",
    "react-intl": "6.2.5",
    "react-use": "17.4.0",
    "styled-components": "5.3.6",
    "typescript": "4.9.4",
    "url-loader": "4.1.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-intl": "^3.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "dotenv": "^16.0.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "next-compose-plugins": "^2.2.1",
    "ts-jest": "^29.0.3"
  }
}
