    "pino": "^9.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.52.1",
    "react-intl": "^6.6.6",
    "react-use": "^17.5.0",
    "url-loader": "4.1.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
